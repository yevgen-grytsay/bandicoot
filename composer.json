{
  "name": "yevgen-grytsay/bandicoot",
  "description": "Data transformation",
  "minimum-stability": "stable",
  "keywords": [
    "transform", "xml"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Yevgen Grytsay",
      "email": "yevgen_grytsay@mail.ru"
    }
  ],
  "require": {
    "php": ">=5.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.6",
    "symfony/property-access": "~2.7",
    "fzaninotto/faker": "1.5.0"
  },
  "autoload": {
    "psr-4": {
      "YevgenGrytsay\\Bandicoot\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "YevgenGrytsay\\Bandicoot\\Tests\\": "tests/"
    }
  },
  "non-feature-branches": [
    "exp-refactor"
  ]
}
